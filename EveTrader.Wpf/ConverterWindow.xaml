<Window x:Class="EveTrader.Wpf.ConverterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Converting data ..." Height="300" Width="300"
        xmlns:vm="clr-namespace:EveTrader.Core.Visual.ViewModel;assembly=EveTrader.Core.Visual"
        xmlns:ctrl="clr-namespace:EveTrader.Wpf.Controls">
    <Grid>
        <ProgressBar VerticalAlignment="Center" HorizontalAlignment="Center" Maximum="{Binding Objects}" Value="{Binding CurrentObject}" Width="200" Height="20"/>
        <Label Content="Test" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button HorizontalAlignment="Center" VerticalAlignment="Bottom" IsEnabled="{Binding Finished}" Content="OK" Width="75" />
    </Grid>
</Window>
